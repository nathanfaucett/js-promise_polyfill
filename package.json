  "readmeFilename": "README.md",
  "devDependencies": {
    "@nathanfaucett/task": "^0.0.5",
    "gulp-jsbeautifier": "^2.0.3",
    "gulp-jshint": "^2.0.1",
    "jshint": "^2.9.3",
    "tap-spec": "^4.1.0",
    "tape": "^4.2.0",
    "zuul": "^3.6.0"
  },
  "dependencies": {
    "@nathanfaucett/apply": "0.0.1",
    "@nathanfaucett/fast_slice": "^0.0.1",
    "@nathanfaucett/is_array": "^0.0.1",
    "@nathanfaucett/is_function": "^0.0.1",
    "@nathanfaucett/is_null": "^0.0.1",
    "@nathanfaucett/is_object": "^0.0.1",
