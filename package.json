{
  "name": "promise_polyfill",
  "version": "0.0.1",
  "description": "promise polyfill for the browser and node.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/nathanfaucett/promise_polyfill.git"
  },
  "main": "src/index.js",
  "scripts": {
    "test": "npm run test-node",
    "test-node": "node ./test | ./node_modules/.bin/tap-spec",
    "test-node-raw": "node ./test",
    "test-browser-local": "zuul --local 9999 --ui tape -- ./test"
  },
  "keywords": [
    "promise",
    "promise.js",
    "promise_polyfill",
    "promise_polyfill.js"
  ],
  "author": "Nathan Faucett",
  "license": "MIT",
  "readmeFilename": "README.md",
  "devDependencies": {
      "grunt": "^0.4.5",
      "grunt-contrib-jshint": "^0.11.3",
      "grunt-jsbeautifier": "^0.2.10",
      "tap-spec": "^4.1.0",
      "tape": "^4.2.0",
      "zuul": "^3.6.0"
  },
  "dependencies": {
    "is_null": "git://github.com/nathanfaucett/is_null.git",
    "create_store": "git://github.com/nathanfaucett/create_store.git",
    "fast_slice": "git://github.com/nathanfaucett/fast_slice.git",
    "is_array": "git://github.com/nathanfaucett/is_array.git",
    "is_object": "git://github.com/nathanfaucett/is_object.git",
    "is_function": "git://github.com/nathanfaucett/is_function.git"
  }
}
